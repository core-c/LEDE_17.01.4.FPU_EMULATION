
config system
	option hostname 'homeYun'
	option zonename 'Europe/Amsterdam'
	option timezone 'CET-1CEST,M3.5.0,M10.5.0/3'
	option log_proto 'udp'
	option conloglevel '8'
	option cronloglevel '8'

config timeserver 'ntp'
	list server '0.openwrt.pool.ntp.org'
	list server '1.openwrt.pool.ntp.org'
	list server '2.openwrt.pool.ntp.org'
	list server '3.openwrt.pool.ntp.org'
	option enabled '1'

config rngd
	option enabled '1'
	option device '/dev/urandom'
	option fill_watermark '4000'

config button
	option button 'wps'
	option action 'pressed'
	option handler '/usr/bin/wifi-reset-button-pressed'

config button
	option button 'wps'
	option action 'released'
	option handler '/usr/bin/wifi-reset-button-released'

config led
	option name 'LinkUp'
	option sysfs 'arduino:blue:wlan'
	option default '1'
	option trigger 'netdev'
	option dev 'wlan0'
	option mode 'link'

config led
	option sysfs 'arduino:white:usb'
	option default '1'
	option trigger 'netdev'
	option dev 'eth1'
	option mode 'tx'
	option name 'eth1tx'
